
@{
    ViewBag.Title = "Trang chủ sinh viên";
    Layout = "~/Areas/Student/Views/Shared/_LayoutStudent.cshtml";
}

<div class="col-sm-10 clear">
    <div class="content-body">
        @RenderPage("~/Views/Shared/_Logout.cshtml")
        <div class="panel-group">
            <div class="panel panel-primary">
                <div class="panel-heading"><h2>Trang chủ Student</h2></div>
                <div class="panel-body">
                    <h4>- Đây là trang chủ của Student với username = @Session["userName"], branchID = @Session["branchID"], courseID = @Session["courseID"] </h4>
                </div>
                <div class="panel-footer">Trang index chỉ để coi cho vui!</div>
            </div>
        </div>
    </div>
</div>
